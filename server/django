server {
    listen 80;
    server_name ip.ad.re.ss.he.re;

    location / {
        # django running in uWSGI
        uwsgi_pass unix:///run/uwsgi/app/django/socket;
        include uwsgi_params;
        uwsgi_read_timeout 300s;
        client_max_body_size 32m;
    }

    location /static/ {
        autoindex on;
        alias    /absolute/path/here/demo/static/;
    }
}
